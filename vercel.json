{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/$1",
        "has": [
          {
            "type": "host",
            "value": "vercel.app"
          }
        ]
      },
      {
        "routes": [
          {
            "src": "/[^.]+",
            "dest": "/"
          }
        ]
      }
    ]
  }